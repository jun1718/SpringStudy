package com.spring.basic.ex04;

import javax.annotation.Resource;

public class Book {
	
	private Paper paper;

//	Book () {
//		
//	}
	

	Book (Paper paper) {
		this.paper = paper;
	}
	public Paper getPaper() {
		return paper;
	}

	@Resource(name = "paper2")
//	@Autowired
//	@Qualifier("paper2")
	public void setPaper(Paper paper) {
		this.paper = paper;
	}
	
	

}
